{
    "id": "amq-jdbc-sample-build",
    "kind": "BuildConfig",
    "apiVersion": "v1beta1",
    "parameters": {
        "source" : {
            "type" : "Git",
            "git" : {
                "uri": "git://github.com/welshstew/jboss-amq-jdbc-dockerfile.git"
            }
        },
        "strategy": {
            "type": "S2I",
            "stiStrategy": {
            "builderImage": "registry.access.redhat.com/jboss-amq-6/amq62-openshift"
            }
        },
        "output": {
            "imageTag": "welshstew/amq-jdbc-sample:latest",
            "registry": "172.121.17.1:5001"
        },
    },
    "secret": "secret101",
    "labels": {
        "name": "amq-jdbc-sample-build"
    }
}